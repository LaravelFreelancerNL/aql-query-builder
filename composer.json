{
  "name": "laravel-freelancer-nl/fluentaql",
  "description": "PHP AQL Query Builder",
  "keywords": [
    "fluentaql",
    "arangodb",
    "aql",
    "aql query",
    "aql query builder"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laravel Freelancer",
      "email": "info@laravel-freelancer.nl"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6",
    "mockery/mockery": "^1.2",
    "nikic/php-parser": "^4.2@dev",
    "phpmd/phpmd": "dev-master",
    "phpstan/phpstan": "^0.12.0@dev",
    "phpunit/phpunit": "8.*",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.0",
    "triagens/arangodb": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "LaravelFreelancerNL\\FluentAQL\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "repositories": [],
  "minimum-stability": "dev"
}
